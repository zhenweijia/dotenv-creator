{
  "name": "dotenv-creator",
  "displayName": "DotEnv Creator",
  "description": "Create .env files from templates with one click",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "env-creator.createEnv",
        "title": "Create .env from Template"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhenwei.jia/dotenv-creator.git"
  },
  "bugs": {
    "url": "https://github.com/zhenwei.jia/dotenv-creator/issues"
  },
  "homepage": "https://github.com/zhenwei.jia/dotenv-creator#readme"
}